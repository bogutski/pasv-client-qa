{
  "name": "wdio-sept",
  "version": "0.1.0",
  "description": "Supper test",
  "main": "index.js",
  "scripts": {
    "test": "wdio wdio.conf.js --inspect",

    "test:user:login:design": "wdio wdio.conf.js --spec test/specs/user/login/loginDesignUserLogin.js --inspect",
    "test:user:login:password:design": "wdio wdio.conf.js --spec test/specs/user/login/loginUserPasswordDesign.js --inspect",
    "test:user:login:password:func": "wdio wdio.conf.js --spec test/specs/user/login/loginPasswordFunc.js --inspect",

    "test:register": "wdio wdio.conf.js --spec test/examples/pages/mainPage.js --inspect",
    "test:user:register:redirect": "wdio wdio.conf.js --spec test/specs/user/registration/redirect.js --inspect",
    "test:user:register:submitButton:disabled:realName": "wdio wdio.conf.js --spec test/specs/user/registration/submit_button_func/negative/realNameEmptyAndIncorreect.js --inspect",
    "test:user:register:aboutField:func": "wdio wdio.conf.js --spec test/specs/user/registration/aboutFieldFunc.js --inspect",
    "test:user:register:submitButton:enabled": "wdio wdio.conf.js --spec test/specs/user/registration/submit_button_func/positive/submitButtonEnabled.js --inspect",
    "test:user:registration:cellPhoneNumber:design": "wdio wdio.conf.js --spec test/specs/user/registration/cellPhoneNumberDesign.js --inspect",
    "test:user:forgotPassword:email:func": "wdio wdio.conf.js --spec ./test/specs/user/forgotPassword/user_forgotPas_emailFunc.js  --inspect",

    "test:pages:home:general": "wdio wdio.conf.js --spec test/specs/pages/homePage/general.js --inspect",
    "test:pages:home:func": "wdio wdio.conf.js --spec test/specs/pages/homePage/homePageFunc.js --inspect",
    "test:pages:home:header:design": "wdio wdio.conf.js --spec test/specs/pages/homePage/homePageHeaderDesign.js --inspect",
    "test:pages:home:footer:design": "wdio wdio.conf.js --spec test/specs/pages/homePage/homePageFooterDesign.js --inspect",

    "test:dairy:func": "wdio wdio.conf.js --spec test/specs/diary/diaryFunc.js --inspect"
  },
  "author": "Viktor Bogutskii",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@wdio/cli": "5.13.1",
    "@wdio/dot-reporter": "5.12.1",
    "@wdio/local-runner": "5.13.1",
    "@wdio/mocha-framework": "5.12.1",
    "@wdio/selenium-standalone-service": "5.12.1",
    "@wdio/spec-reporter": "5.12.1",
    "@wdio/sync": "5.13.0",
    "axios": "0.19.0",
    "babel-eslint": "^10.0.3",
    "chai": "4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "3.0.5",
    "lint-staged": "9.3.0",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
